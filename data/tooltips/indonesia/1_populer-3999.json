    {
        "id": 1,
        "keyword": "sholat",
        "description_short": "Ibadah wajib umat Islam",
        "description_long": "Sholat adalah ibadah wajib yang dilakukan lima kali sehari sebagai bentuk ketaatan kepada Allah."
    },
    {
        "id": 2,
        "keyword": "puasa",
        "description_short": "Ibadah menahan diri dari makan dan minum",
        "description_long": "Puasa adalah ibadah wajib di bulan Ramadhan serta sunnah di hari-hari tertentu, dilakukan dengan menahan diri dari makan, minum, dan hawa nafsu dari fajar hingga maghrib."
    },
    {
        "id": 3,
        "keyword": "zakat",
        "description_short": "Kewajiban mengeluarkan sebagian harta",
        "description_long": "Zakat adalah kewajiban bagi umat Islam untuk mengeluarkan sebagian harta tertentu kepada golongan yang berhak menerima, sebagai bentuk kepedulian sosial dan penyucian harta."
    },
    {
        "id": 4,
        "keyword": "haji",
        "description_short": "Ibadah ke Baitullah bagi yang mampu",
        "description_long": "Haji adalah rukun Islam kelima, diwajibkan bagi setiap Muslim yang mampu secara finansial dan fisik, dilakukan di kota Makkah dengan serangkaian ibadah tertentu."
    },
    {
        "id": 5,
        "keyword": "umrah",
        "description_short": "Ibadah ke Tanah Suci dengan rukun lebih ringan",
        "description_long": "Umrah adalah ibadah sunnah yang dilakukan di Makkah dan memiliki kemiripan dengan haji, namun tidak memiliki waktu tertentu dan tanpa ritual wukuf di Arafah."
    },
    {
        "id": 6,
        "keyword": "tauhid",
        "description_short": "Konsep keesaan Allah dalam Islam",
        "description_long": "Tauhid adalah konsep fundamental dalam Islam yang menegaskan keesaan Allah. Tauhid terbagi menjadi tiga: Tauhid Rububiyah (kepercayaan bahwa Allah adalah satu-satunya pencipta), Tauhid Uluhiyah (hanya Allah yang berhak disembah), dan Tauhid Asma wa Sifat (mengimani nama-nama dan sifat Allah yang Maha Sempurna)."
    },
    {
        "id": 7,
        "keyword": "hadits",
        "description_short": "Perkataan, perbuatan, dan persetujuan Nabi Muhammad",
        "description_long": "Hadits adalah catatan tentang perkataan, perbuatan, dan persetujuan Nabi Muhammad SAW yang menjadi sumber hukum Islam kedua setelah Al-Qur'an."
    },
    {
        "id": 8,
        "keyword": "fiqh",
